<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" fx:id="customerMain" prefHeight="500" prefWidth="1000.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.221" fx:controller="views.CustomerMainController">
   <children>
      <Label layoutX="116.0" layoutY="14.0" text="Customers">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <TableView fx:id="customerTable" layoutX="260" layoutY="70" prefHeight="340" prefWidth="500">
        <columns>
          <TableColumn fx:id="customerId" prefWidth="50" text="ID" />
          <TableColumn fx:id="customerName" prefWidth="370" text="Customer" />
        </columns>
      </TableView>

      <Button layoutX="350" layoutY="430" mnemonicParsing="false" onAction="#handleAddButton" text="Add" />
      <Button layoutX="480" layoutY="430" mnemonicParsing="false" onAction="#handleModifyButton" text="Modify" />
      <Button layoutX="610" layoutY="430" mnemonicParsing="false" onAction="#handleDeleteButton" text="Delete" />
      <Button layoutX="490" layoutY="460" mnemonicParsing="false" onAction="#handleBackButton" text="Back" />
   </children>
</AnchorPane>
