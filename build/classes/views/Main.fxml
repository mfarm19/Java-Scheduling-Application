<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500" prefWidth="1000" xmlns="http://javafx.com/javafx/8.0.221" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.MainController">
   <children>
      <Label layoutX="400" layoutY="20" text="Scheduler Options">
         <font>
            <Font size="24" />
         </font>
      </Label>
      <Button layoutX="400" layoutY="100" mnemonicParsing="false" onAction="#handleCustomerButton" prefWidth="200" text="Customer Manager" />
      <Button layoutX="400" layoutY="150" mnemonicParsing="false" onAction="#handleAppointmentButton" prefWidth="200" text="Appointment Manager" />
      <Button layoutX="400" layoutY="200" mnemonicParsing="false" onAction="#handleReportsButton" prefWidth="200" text="Reports" />
      <Button layoutX="400" layoutY="250" mnemonicParsing="false" onAction="#handleLogsButton" prefWidth="200" text="Log.txt" />
   </children>
</AnchorPane>
